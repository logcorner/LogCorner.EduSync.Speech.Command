{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "HubUrl": "https://localhost:5001/logcornerhub",
  "allowedOrigins": "http://localhost:4200",
  "ConnectionStrings": {
    "SpeechDB": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=LogCorner.EduSync.Speech.Data;Integrated Security=True"
  },
  "AzureAdB2C": {
    "Instance": "https://datasynchrob2c.b2clogin.com/tfp/",
    "ClientId": "[ClientId]",
    "CallbackPath": "/signin-oidc",
    "Domain": "[TenantName].onmicrosoft.com",
    "SignUpSignInPolicyId": "B2C_1_SignUpIn",
    "ResetPasswordPolicyId": "B2C_1_PasswordReset",
    "EditProfilePolicyId": "B2C_1_ProfileEdit"
  },
  "SwaggerUI": {
    "TenantName": "[TenantName]",
    "OAuthClientId": "[OAuthClientId]",
    "OAuthClientSecret": "[OAuthClientSecret]"
  },

  "AzureAdConfideantialClient": {
    "ClientId": "[ClientId]",
    "ClientSecret": "[ClientSecret]",
    "TenantId": "[TenantId]"
  }
}